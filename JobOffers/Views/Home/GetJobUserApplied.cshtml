@model IEnumerable<JobOffers.Models.ApplyForJob>
@{
    ViewBag.Title = "GetJobUserApplied";
}

<h2>GetJobUserApplied</h2>


<table class="table table-hover table-bordered ">
    <thead>
        <tr>
            <td>Job Name</td>
            <td>Job Data</td>
            <td>Job Category</td>
        </tr>
    </thead>
    <tbody>
        @foreach (var job in Model)
        {
            
            <tr>
                <td>
                    @job.job.JobTitle
                </td>
                <td>
                    @job.ApplyDate
                </td>

                <td>@job.job.Category.CategoryName</td>
            </tr>

        }
    </tbody>

</table>